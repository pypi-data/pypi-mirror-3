<?xml version="1.0" encoding="UTF-8" ?>
<GoogleCustomizations xmlns:tal="http://xml.zope.org/namespaces/tal">
    <CustomSearchEngine id="d6kv20cgnc0"
        creator="008685122749687227183"
        version="1.0"
        keywords=""
        language="en"
        encoding="UTF-8"
        enable_suggest="true"
        top_refinements="5"
        tal:attributes="id view/google_cse_id;
                creator view/creator;
                keywords view/keywords;
                language view/language;
                enable_suggest view/enable_suggest"
        >
        <Title tal:content="here/Title"> Title </Title>
        <Description tal:content="here/Description"> Description </Description>
        <Context>
           <Facet tal:repeat="label view/get_labels" tal:condition="view/labelkeywords" >
                <FacetItem>
                   <Label name="project_sites"
                          mode="FILTER"
                          weight="0.7"
                          tal:attributes="name label/label_name">
                      <Rewrite/>
                      <IgnoreBackgroundLabels>false</IgnoreBackgroundLabels>
                  </Label>
                  <Title tal:content="label/label_title">Cuba</Title>
               </FacetItem>
           </Facet>
           <BackgroundLabels>
                <Label name="_cse_d6kv20cgnc0" mode="FILTER" weight="0.7"
                        tal:attributes="name string:_cse_${view/google_cse_id}"/>
                <Label name="_cse_exclude_d6kv20cgnc0" mode="ELIMINATE" weight="0.7"
                        tal:attributes="name string:_cse_exclude_${view/google_cse_id}"/>
          </BackgroundLabels>
        </Context>
        <LookAndFeel nonprofit="true"
           tal:attributes="nonprofit view/ad_status">
        </LookAndFeel>
