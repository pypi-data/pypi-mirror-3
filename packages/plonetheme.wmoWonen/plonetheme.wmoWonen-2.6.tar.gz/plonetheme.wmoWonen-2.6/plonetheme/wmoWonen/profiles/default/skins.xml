<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="WMOWonen Theme">

 <object name="plonetheme_wmoWonen_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.wmoWonen:skins/plonetheme_wmoWonen_custom_images"/>
 <object name="plonetheme_wmoWonen_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.wmoWonen:skins/plonetheme_wmoWonen_custom_templates"/>
 <object name="plonetheme_wmoWonen_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.wmoWonen:skins/plonetheme_wmoWonen_styles"/>

 <skin-path name="WMOWonen Theme" based-on="INTK Base">
  <layer name="plonetheme_wmoWonen_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_wmoWonen_custom_templates"
     insert-after="plonetheme_wmoWonen_custom_images"/>
  <layer name="plonetheme_wmoWonen_styles"
     insert-after="plonetheme_wmoWonen_custom_templates"/>
 </skin-path>

</object>
