[
    {
        "name": "Cisco | IOS | Chassis | Fan | Fan Failed #1 (SYSLOG)",
        "description": "%FAN-3-FAN_FAILED: Fan 1 had a rotation error reported.",
        "event_class__name": "Chassis | Fan | Fan Failed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%FAN-3-FAN_FAILED: (?P<name>Fan \\S+|Fans) had a rotation error reported"
            }
        ]
    },
    
    {
        "name": "Cisco | IOS | Chassis | Fan | Fan Recovered #1 (SYSLOG)",
        "description": "%FAN-3-FAN_OK: Fan 1 had earlier reported a rotation error. It is ok now",
        "event_class__name": "Chassis | Fan | Fan Recovered",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%FAN-3-FAN_OK: (?P<name>Fan \\S+) had earlier reported a rotation error\\. It is ok now"
            }
        ]
    },

    {
