[
    {
        "name": "Juniper | ScreenOS | Network | BGP | Backward Transition (SNMP)",
        "description": "NETSCREEN-BGP4-MIB::nsBgpEstablished, bgp-backwardtransition",
        "event_class__name": "Network | BGP | Backward Transition",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Juniper\\.ScreenOS$"
            },
            {
                "key_re": "^NETSCREEN-TRAP-MIB::netscreenTrapType\\.0$",
                "value_re": "^bgp-backwardtransition$"
            },
            {
                "key_re": "^NETSCREEN-VR-BGP4-MIB::nsVrBgpPeerIdentifier$",
                "value_re": "^(?P<peer__int_to_ip>\\S+)$"
            },
            {
                "key_re": "^NETSCREEN-VR-BGP4-MIB::nsVrBgpPeerState$",
                "value_re": "^(?P<state__enum__bgp_state>\\S+)$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^NETSCREEN-BGP4-MIB::nsBgpEstablished$"
            }
        ]
    },
    
    {
        "name": "Juniper | ScreenOS | Network | BGP | Established (SNMP)",
        "description": "NETSCREEN-BGP4-MIB::nsBgpEstablished, bgp-established",
        "event_class__name": "Network | BGP | Established",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Juniper\\.ScreenOS$"
            },
            {
                "key_re": "^NETSCREEN-TRAP-MIB::netscreenTrapType\\.0$",
                "value_re": "^bgp-established$"
            },
            {
                "key_re": "^NETSCREEN-VR-BGP4-MIB::nsVrBgpPeerIdentifier$",
                "value_re": "^(?P<peer__int_to_ip>\\S+)$"
            },
            {
                "key_re": "^NETSCREEN-VR-BGP4-MIB::nsVrBgpPeerState$",
                "value_re": "^established$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^NETSCREEN-BGP4-MIB::nsBgpEstablished$"
            }
        ]
    },

    {
        "name": "Juniper | ScreenOS | Network | BGP | Peer State Changed (SYSLOG)",
        "description": "fw-1-m9: NetScreen device_id=fw-1-m9  [Root]system-information-00542: BGP peer 10.108.251.21 changed to Idle state (2011-08-17 00:01:14)",
        "event_class__name": "Network | BGP | Peer State Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Juniper\\.ScreenOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "system-information-00542: BGP peer (?P<peer>\\S+) changed to (?P<to_state__enum__bgp_state>\\S+) state"
            }
        ]
    }
]