<?xml version="1.0" encoding="UTF-8"?><!-- test that only finalize markup in the invokingstate runs.  test234sub1.scxml will
return 2 in _event.data.aParam, while test234sub2.scxml doesn't return any events.  So
var1 should have value 2 but var2 should still be set to 1  --><scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" initial="p0" datamodel="python">
<datamodel>
  <data id="Var1" expr="1"/>
  <data id="Var2" expr="1"/>
  </datamodel>
<parallel id="p0">
    <onentry>
    <send event="timeout" delay="3s"/>
    </onentry>   
    <transition event="timeout" target="fail"/>  
    
   <state id="p01">
    <invoke type="http://www.w3.org/TR/scxml/" src="file:test234sub1.scxml">
      <finalize>
        <assign location="Var1" expr="_event.data.get('aParam')"/>
        </finalize>
        </invoke>
      
      <transition event="childToParent" cond="Var1==2" target="s1"/>
       <transition event="childToParent" target="fail"/>
    </state> 

   <state id="p02">
     <invoke type="http://www.w3.org/TR/scxml/" src="file:test234sub2.scxml">
      <finalize>
        <assign location="Var2" expr="_event.data.get('aParam')"/>
         </finalize>
      </invoke>
  </state>

</parallel>    

  
<state id="s1">
  <transition cond="Var2==1" target="pass"/>
  <transition target="fail"/>
  </state>
  
<final id="pass"><onentry><log label="Outcome" expr="'pass'"/></onentry></final>
<final id="fail"><onentry><log label="Outcome" expr="'fail'"/></onentry></final>
</scxml>