[
    {
        "name": "MikroTik | RouterOS | Network | Link | Link Down (SYSLOG)",
        "description": "interface,info ether3-TRANSIT link down",
        "event_class__name": "Network | Link | Link Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^MikroTik\\.RouterOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?P<interface>\\S+) link down$"
            }
        ]
    },

    {
        "name": "MikroTik | RouterOS | Network | Link | Link Up (SYSLOG)",
        "description": "interface,info ether3-TRANSIT link up (speed 100M, full duplex) ",
        "event_class__name": "Network | Link | Link Up",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^MikroTik\\.RouterOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?P<interface>\\S+) link up \\(speed (?P<speed>\\S+), (?P<duplex>\\S+ duplex)\\)"
            }
        ]
    }
]
