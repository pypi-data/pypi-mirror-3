<?xml version="1.0" encoding="UTF-8"?><!-- a duplicate of test205, but this time we're testing that the specified data is included.  --><scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" initial="s0" datamodel="python">
<datamodel>
   <data id="Var1"/>
   </datamodel>
   
<state id="s0">
  <onentry>
   <send event="event1">
     <param name="aParam" expr="1"/>
     </send>
   <send event="timeout"/>
    </onentry>
 
 
  <transition event="event1" target="s1">
     <assign location="Var1" expr="_event.data.get('aParam')"/>
     </transition>
  <transition event="*" target="fail"/>
 </state>

<state id="s1">
  <transition cond="Var1==1" target="pass"/>
  <transition target="fail"/>
  </state> 

<final id="pass"><onentry><log label="Outcome" expr="'pass'"/></onentry></final>
<final id="fail"><onentry><log label="Outcome" expr="'fail'"/></onentry></final>

</scxml>