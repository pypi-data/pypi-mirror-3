<?xml version="1.0"?>
<object name="BastionPayment"
   meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title">BastionPayment</property>
 <property name="description">BastionPayment content type.</property>
 <property name="content_icon">payment.gif</property>
 <property name="content_meta_type">BastionPayment</property>
 <property name="product">BastionBanking</property>
 <property name="factory"></property>
 <property name="immediate_view">bastionpayment_view</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types"></property>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="bastionpayment_view"/>
 <alias from="merchant" to="bastionpayment_merchant_info"/>
 <alias from="sharing" to="folder_localrole_form"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}" visible="True">
  <permission value="View"/>
 </action>
 <action title="Merchant" action_id="merchant" category="object" condition_expr=""
    url_expr="string:${object_url}/merchant" visible="True">
  <permission value="View"/>
 </action>
</object>
