[
    {
        "name": "Cisco | IOS | Config | Config Changed #1 (SNMP)",
        "description": "CISCO-CONFIG-MAN-MIB::ciscoConfigManMIBNotificationPrefix trap",
        "event_class__name": "Config | Config Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^CISCO-CONFIG-MAN-MIB::ciscoConfigManMIBNotificationPrefix$"
            }
        ]
    },
    
    {
        "name": "Cisco | IOS | Config | Config Changed #2 (SNMP)",
        "description": "ENTITY-MIB::entConfigChange",
        "event_class__name": "Config | Config Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^ENTITY-MIB::entConfigChange$"
            }
        ]
    },
    
    {
        "name": "Cisco | IOS | Config | Config Synced #1 (SYSLOG)",
        "description": "%C4K_REDUNDANCY-5-CONFIGSYNC: The startup-config has been successfully synchronized to the standby supervisor",
        "event_class__name": "Config | Config Synced",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%C4K_REDUNDANCY-5-CONFIGSYNC: The startup-config has been successfully synchronized to the standby supervisor"
            }
        ]
    },
    
    {
        "name": "Cisco | IOS | Config | Config Synced #2 (SYSLOG)",
        "description": "%PFINIT-SP-5-CONFIG_SYNC: Sync'ing the startup configuration to the standby Router.",
        "event_class__name": "Config | Config Synced",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%PFINIT-SP-5-CONFIG_SYNC: Sync'ing the (?:startup|private) configuration to the standby Router."
            }
        ]
    },

    {
        "name": "Cisco | IOS | Config | Config Sync Failed (SYSLOG)",
        "description": "%PFINIT-SP-1-CONFIG_SYNC_FAIL: Sync'ing the startup configuration to the standby Router FAILED",
        "event_class__name": "Config | Config Synced",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%PFINIT-SP-1-CONFIG_SYNC_FAIL(?:_RETRY)?: Sync'ing the (?P<config>startup|private) configuration to the standby Router FAILED.*"
            }
        ]
    },
    
    {
        "name": "Cisco | IOS | Config | Exiting Configuration Mode #1 (SYSLOG)",
