blockdiag {
   orientation = portrait;
   node_width = 180;
   default_fontsize = 20;
   n [label = "baseNodes", fontsize = 16];
   w [label = "writerNodes"];
   f [label = "nodeFactory"];
   p [label = "parser"];
   c [label = "buml-to-xml"];

   c -> p [label = "calls"];
   p -> f [label = "uses"];
   f -> w [label = "instantiates"];
   w -> n [label = "extends"];
}

