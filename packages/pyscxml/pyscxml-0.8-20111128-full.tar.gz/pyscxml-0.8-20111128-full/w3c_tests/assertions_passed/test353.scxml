<?xml version="1.0" encoding="UTF-8"?><!-- test that _event.language matches sending sessions datamodel --><scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" initial="s0" datamodel="python">

  
<state id="s0">
   <onentry>
     <send delay="5s" event="timeout"/>
    <send type="http://www.w3.org/TR/scxml/#SCXMLEventProcessor" event="s0Event"/>  
     </onentry>
  <transition event="s0Event" cond=" _event.language == 'python'" target="pass"/>
  <transition event="*" target="fail">
    </transition>

</state>

   <final id="pass"><onentry><log label="Outcome" expr="'pass'"/></onentry></final>
   <final id="fail"><onentry><log label="Outcome" expr="'fail'"/></onentry></final>

</scxml>