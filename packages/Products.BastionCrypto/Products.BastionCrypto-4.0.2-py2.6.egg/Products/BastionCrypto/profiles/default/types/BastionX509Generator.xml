<?xml version="1.0"?>
<object name="BastionX509Generator"
   meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title">BastionX509Generator</property>
 <property name="description">BastionX509Generator content type.</property>
 <property name="content_icon">x509.gif</property>
 <property name="content_meta_type">BastionX509Generator</property>
 <property name="product">BastionCrypto</property>
 <property name="factory">manage_addBastionX509Generator</property>
 <property name="immediate_view">X509_edit_form</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="X509_edit_form"/>
 <alias from="index.html" to="(dynamic view)"/>
 <alias from="properties" to="base_metadata"/>
 <alias from="sharing" to="folder_localrole_form"/>
 <alias from="view" to="(selected layout)"/>
 <action title="Generate" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}" visible="True">
  <permission value="View"/>
 </action>
 <action title="Format" action_id="format" category="object" condition_expr=""
         url_expr="string:${object_url}/X509_format_form" visible="True">
  <permission value="View"/>
 </action>
</object>
