#. Default: "Configure the parameters for this slideshow folder."
#: ./browser.py:269
msgid "description_slideshow_settings_form"
msgstr "Vorgaben für diesen Diaschauordner anpassen."

#. Default: "Slideshow folder settings"
#: ./browser.py:268
msgid "heading_slideshow_settings_form"
msgstr "Diaschauordner Einstellungen"

#. Default: "If selected, user must click slideshow to start."
#: ./interfaces.py:88
msgid "help_click_to_start"
msgstr "Wenn markiert, muss der Benutzer klicken um die Diaschau zu starten."

#. Default: "Skips the slide transition when a user clicks on a thumbnail."
#: ./interfaces.py:71
msgid "help_fast_mode"
msgstr "Lässt den Bildübergang weg, wenn der Benutzer auf ein Vorschaubild klickt."

#. Default: "Select the image size to display."
#: ./interfaces.py:27
msgid "help_image_size"
msgstr "Angezeigte Bildgrösse wählen."

#. Default: "Link images to full-sized version"
#: ./interfaces.py:104
msgid "help_link_images"
msgstr "Bilder zur Originalgrösse verlinken"

#. Default: "Repeats the slideshow once it has finished."
#: ./interfaces.py:98
msgid "help_loop_slideshow"
msgstr "Wiederholt die Diaschau."

#. Default: "Shows slides in a random order."
#: ./interfaces.py:93
msgid "help_random_order"
msgstr "Zeigt die Bilder in einer zufälligen Reihenfolge."

#. Default: "Include captions below the image."
#: ./interfaces.py:76
msgid "help_show_captions"
msgstr "Zeige Legende unterhalb des Bildes an."

#. Default: "Include play/pause/forward/back controller in the slideshow."
#: ./interfaces.py:82
msgid "help_show_controller"
msgstr "Zeige abspielen/pause/weiter/zurück Kontrollelemente in der Diaschau."

#. Default: "Time in seconds between slide transitions"
#: ./interfaces.py:41
msgid "help_slide_duration"
msgstr "Zeit in Sekunden bis zum nächsten Bildwechsel"

#. Default: "Height of the slideshow in pixels.  Images will be shrunk to fit within the dimensions of the slideshow."
#: ./interfaces.py:46
msgid "help_slideshow_height"
msgstr "Höhe der Diaschau in Pixeln. Die Bilder werden verkleinert um in den Bereich der Diaschau zu passen."

#. Default: "Width of the slideshow in pixels.  Images will be shrunk to fit within the dimensions of the slideshow."
#: ./interfaces.py:36
msgid "help_slideshow_width"
msgstr "Breite der Diaschau in Pixeln. Die Bilder werden verkleinert um in den Bereich der Diaschau zu passen."

#. Default: "Include navigation thumbnails below the image."
#: ./interfaces.py:65
msgid "help_thumbnail_mode"
msgstr "Vorschaubilder für Navigation anzeigen."

#. Default: "Time in seconds for a slide transition to take place."
#: ./interfaces.py:59
msgid "help_transition_time"
msgstr "Zeit in Sekunden für Bildübergang."

#. Default: "Click to start slideshow"
#: ./interfaces.py:87
msgid "label_click_to_start"
msgstr "Klicken um Diaschau zu starten"

#. Default: "Fast Mode"
#: ./interfaces.py:70
msgid "label_fast_mode"
msgstr "Schneller Bildwechsel"

#. Default: "Image size"
#: ./interfaces.py:26
msgid "label_image_size"
msgstr "Bildgrösse"

#. Default: "Link images"
#: ./interfaces.py:103
msgid "label_link_images"
msgstr "Bilder verlinken"

#. Default: "Loop Slideshow"
#: ./interfaces.py:97
msgid "label_loop_slideshow"
msgstr "Wiederhole Diaschau"

#. Default: "Random order"
#: ./interfaces.py:92
msgid "label_random_order"
msgstr "Zufällige Reihenfolge"

#. Default: "Show Captions"
#: ./interfaces.py:75
msgid "label_show_captions"
msgstr "Bildlegende"

#. Default: "Show Controller"
#: ./interfaces.py:81
msgid "label_show_controller"
msgstr "Kontrollelemente anzeigen"

#. Default: "Slide Duration"
#: ./interfaces.py:40
msgid "label_slide_duration"
msgstr "Anzeigedauer eines Bildes"

#. Default: "Slideshow Height"
#: ./interfaces.py:44
msgid "label_slideshow_height"
msgstr "Höhe der Diaschau"

#. Default: "Slideshow Width"
#: ./interfaces.py:34
msgid "label_slideshow_width"
msgstr "Breite der Diaschau"

#. Default: "Thumbnail Mode"
#: ./interfaces.py:64
msgid "label_thumbnail_mode"
msgstr "Vorschau Modus"

#. Default: "Transition Time"
#: ./interfaces.py:58
msgid "label_trasition_time"
msgstr "Dauer des Bildübergangs"

#. Default: "You must add <em>and publish</em> images within this folder for them to show up in this slideshow."
#: ./skins/slideshowfolder/slideshow_macros.pt:27
msgid "message_need_images"
msgstr "Sie müssen Bilder hinzufügen <em>und publizieren</em> damit sie in dieser Diaschau angezeigt werden."

#. Default: "This folder is now designated a slideshow."
#: ./browser.py:231
msgid "message_slideshow_created"
msgstr "Dieser Ordner ist nun eine Diaschau."

#. Default: "Completely removed slideshow capabilities."
#: ./browser.py:261
msgid "message_slideshow_removed"
msgstr "Die Diaschau-Funktionalität wurde von diesem Ordner entfernt."

#. Default: "There are currently no images in this slideshow."
#: ./skins/slideshowfolder/slideshow_macros.pt:23
msgid "text_no_images"
msgstr "Im Moment befinden sich keine Bilder in dieser Diaschau."

#. Default: "First [Shift + Left arrow]"
#: ./browser.py:187
msgid "title_controller_first"
msgstr "Anfang [Shift + Pfeil links]"

#. Default: "Last [Shift + Right arrow]"
#: ./browser.py:191
msgid "title_controller_last"
msgstr "Ende [Shift + Pfeil rechts]"

#. Default: "Next [Right arrow]"
#: ./browser.py:190
msgid "title_controller_next"
msgstr "Weiter [Pfeil rechts]"

#. Default: "Play / Pause [P]"
#: ./browser.py:189
msgid "title_controller_pause"
msgstr "Abspielen / Pause [P]"

#. Default: "Previous [Left arrow]"
#: ./browser.py:188
msgid "title_controller_prev"
msgstr "Zurück [Pfeil links]"

