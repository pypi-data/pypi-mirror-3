<?xml version="1.0"?>
<object name="Zpydoc" meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title"></property>
 <property name="description">An automatic documentation tool.</property>
 <property name="content_icon">zpydoc.gif</property>
 <property name="content_meta_type">Zpydoc</property>
 <property name="product">Zpydoc</property>
 <property name="factory">manage_addZpydoc</property>
 <property name="immediate_view">zpydoc_edit_form</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="ZpydocPackage"/>
 </property>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="zpydoc_view"/>
 <alias from="edit" to="zpydoc_edit_form"/>
 <alias from="modules" to="zpydoc_modules_form"/>
 <alias from="view" to="zpydoc_view"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
 <action title="Modules" action_id="modules" category="object" condition_expr=""
    url_expr="string:${object_url}/modules" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
