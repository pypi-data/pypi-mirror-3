  edge [color=gray, arrowType="empty", arrowsize={% if minimal %}0.5{% else %}1{% endif %}];{% for subject in subjects %}
  a_{{ subject.hexhash }} [{% if minimal %}fizedsize=true, width=0.05, height=0.05, {% endif %}label="{% if  minimal %}â€¢{% else %}{{ subject.label|truncatewords:5|escape_dot|safe }}{% endif %}", tooltip="{{ subject.label|escape_dot }}" href="{{ subject.doc_url }}"{% if subject.dcterms_published %}, date="{{ subject.dcterms_published }}"{% endif %}];{% for child in subject.children %}{% if child.display %}
  a_{{ subject.hexhash }} -> a_{{ child.hexhash }} ;{% endif %}{% endfor %}{% endfor %}
