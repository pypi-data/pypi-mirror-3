[project attributes]
proj.directory-list = [{'dirloc': loc('.'),
                        'excludes': [u'bin',
                                     u'docs/coverage',
                                     u'dist',
                                     u'docs/html',
                                     u'lib',
                                     u'bootstrap.py',
                                     u'distribute_setup.py',
                                     u'docs/source/repoze.sphinx.autointerface-0.4',
                                     u'm4us.egg-info',
                                     u'docs/doctrees',
                                     u'COPYING',
                                     u'README.html',
                                     u'lib64',
                                     u'include'],
                        'filter': '*',
                        'include_hidden': False,
                        'recursive': True,
                        'watch_for_changes': True}]
