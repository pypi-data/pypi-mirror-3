<?xml version="1.0"?>
<object name="portal_controlpanel"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="pcommerce.payment.docdata"
        purge="False">

 <configlet title="Docdata configuration"
            action_id="DocdataConfiglet"
            appId="pcommerce.payment.docdata"
            category="Products"
            condition_expr=""
            url_expr="string:${portal_url}/@@docdata-pcommerce-settings"
            visible="True"
            i18n:attributes="title"
            icon_expr="string:++resource++pcommerce_payment_docdata_icon.png">
  <permission>Manage portal</permission>
 </configlet>

</object>
