        "preference": 1000,
        "vars": [
            {
                "name": "name",
                "value": "Standby Supervisor"
            }
        ],
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%(?:C4K_)?REDUNDANCY-3-STANDBY_LOST: Standby processor fault"
            }
        ]
