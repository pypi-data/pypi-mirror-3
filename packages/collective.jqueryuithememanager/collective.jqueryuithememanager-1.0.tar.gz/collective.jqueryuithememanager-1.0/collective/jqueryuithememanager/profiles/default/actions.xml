<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_actions" />
 <object name="jqueryui_panels" meta_type="CMF Action Category">
  <object name="thememanager" meta_type="CMF Action" i18n:domain="collective.jqueryuithememanager">
   <property name="title" i18n:translate="">Theme manager</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/@@collective.jqueryuithememanager-controlpanel</property>
   <property name="icon_expr">string:$portal_url/++resource++jqueryui_icon.jpg</property>
   <property name="permissions">
    <element value="Plone Site Setup: Overview"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
