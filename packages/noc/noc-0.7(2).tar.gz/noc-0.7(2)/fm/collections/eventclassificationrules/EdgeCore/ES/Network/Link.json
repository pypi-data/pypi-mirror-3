[
    {
        "name": "EdgeCore | ES | Network | Link | Link Down (SYSLOG)",
        "description": "Aug 31 13:34:23 test snmp Unit 1, Port 25 link-down notification.",
        "event_class__name": "Network | Link | Link Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^EdgeCore\\.ES$"
            },
            {
                "key_re": "^message$",
                "value_re": "Unit (?P<unit>\\S+), Port\\s+(?P<port>\\d+) link-down notification\\.$"
            }
        ],
        "vars": [
            {
                "name": "interface",
                "value": "='Eth %s/%s' % (unit, port)" 
            }
        ]
    },

    {
        "name": "EdgeCore | ES | Network | Link | Link Up (SYSLOG)",
        "description": "Aug 29 16:27:45 test snmp Unit 1, Port  1 link-up 100M FD notification.",
        "event_class__name": "Network | Link | Link Up",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^EdgeCore\\.ES$"
            },
            {
                "key_re": "^message$",
                "value_re": "Unit (?P<unit>\\S+), Port\\s+(?P<port>\\d+) link-up(?: 10M | 100M | 1G )?(?:FD|HD)? notification\\.$"
            }
        ],
        "vars": [
            {
                "name": "interface",
                "value": "='Eth %s/%s' % (unit, port)" 
            }
        ]
    }

]
