<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US"
  lang="en-US"
  metal:use-macro="here/main_template/macros/master"
  i18n:domain="eea">

<body>

<div metal:fill-slot="content-core">

  <div style="width: 100%">
    <div style="float: left; width: 100%">
      <div style="width: 100%; overflow: auto; text-align: center; border: 1px solid #8CACBB; padding: 2em 0">
      <img tal:attributes="
        src string:${here/absolute_url}/@@graph.png;
        alt string:Relations Graph;
        title string:Relations Graph" />
      </div>
    </div>
    <div style="float: left; width: 48%">
      <tal:viewlet replace="structure here/@@contenttypes.html" />
    </div>
    <div style="float: right; width: 48%">
      <tal:viewlet replace="structure here/@@relations.html" />
    </div>
  </div>

  <div class="visualClear"></div>

</div>

</body>
</html>
