[
    {
        "name": "Force10 | FTOS | Chassis | CPU | CPU Usage Above Threshold (SNMP)",
        "description": "F10-CHASSIS-MIB::chAlarmExdCpuThreshold",
        "event_class__name": "Chassis | CPU | CPU Usage Above Threshold",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Force10\\.FTOS$"
            },
            {
                "key_re": "^F10-CHASSIS-MIB::chAlarmVarString$",
                "value_re": "^CPU_THRESHOLD: Overall (?P<cpu>\\S+) cpu usage above threshold\\."
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^F10-CHASSIS-MIB::chAlarmExdCpuThreshold$"
            }
        ]
    },

    {
        "name": "Force10 | FTOS | Chassis | CPU | CPU Usage Drops Below Threshold (SNMP)",
        "description": "F10-CHASSIS-MIB::chAlarmClrCpuThreshold",
        "event_class__name": "Chassis | CPU | CPU Usage Drops Below Threshold",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Force10\\.FTOS$"
            },
            {
                "key_re": "^F10-CHASSIS-MIB::chAlarmVarString$",
                "value_re": "^CPU_THRESHOLD_CLR: Overall (?P<cpu>\\S+) cpu usage drops below threshold\\."
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^F10-CHASSIS-MIB::chAlarmClrCpuThreshold$"
            }
        ]
    },
    
    {
        "name": "Force10 | FTOS | Chassis | CPU | CoPP Drop (SYSLOG)",
        "description": "Aug 11 01:24:44 UTC: %STKUNIT1-M:CP %KERN-6-INT: CP CPU receiving excessive control plane traffic: will be rate controlled",
        "event_class__name": "Chassis | CPU | CoPP Drop",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Force10\\.FTOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%KERN-6-INT: (?P<cpu>\\S+) CPU receiving excessive control plane traffic: will be rate controlled$"
            }
        ]
    }
]
