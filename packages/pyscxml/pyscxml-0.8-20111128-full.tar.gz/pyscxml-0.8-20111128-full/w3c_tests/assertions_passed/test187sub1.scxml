<?xml version="1.0" encoding="UTF-8"?><!-- exit before the delayed send can execute --><scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" initial="s0" datamodel="python">

  
<state id="s0">
  <onentry>
   <send event="childToParent" target="#_parent" delay="1s"/>

    </onentry>
    
  <transition target="final"/>

 </state>

<final id="final"/>

</scxml>