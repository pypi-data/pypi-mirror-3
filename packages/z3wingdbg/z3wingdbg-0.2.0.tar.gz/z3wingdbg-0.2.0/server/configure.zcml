<configure xmlns="http://namespaces.zope.org/zope">

<!-- http server -->
<utility
    component=".http.factory"
    provides="..interfaces.IDebugServerFactory"
    name="http"
    />
    
<class class=".configuration.DebugServerConfiguration">
  <require
      permission="zope.ManageServices"
      interface="..interfaces.IDebugServerConfiguration"
      set_schema="..interfaces.IDebugServerConfiguration"
      />
</class>      

</configure>
