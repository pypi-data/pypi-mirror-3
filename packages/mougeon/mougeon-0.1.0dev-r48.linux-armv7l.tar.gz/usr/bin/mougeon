#!/usr/bin/python
#THIS IS MOUGEON LAUNCH SCRIPT

import mougeon
import mougeon.bootstrap 

import sys, os

# backup the current dir
current = os.getcwdu()

# change to pyjama package dir
os.chdir(mougeon.__path__[0])

# run mouegon
mougeon.bootstrap.run()

# restore the direction
os.chdir(current)
