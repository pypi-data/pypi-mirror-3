<h1 tal:replace="structure here/manage_page_header">Header</h1>

<h2 tal:define="form_title string:Add Relationship Local Role Manager"
      tal:replace="structure here/manage_form_title">Form Title</h2>

  <p class="form-help">
  Install a relationship local role manager. This plugin will enable
  you to use simple adapter registrations to determine localroles.
  </p>

<form action="manage_addRelationshipLocalRoleManager" method="POST">
  <table>
    <tr>
      <td class="form-label">Id</td>
      <td><input type="text" name="id"></td>
    </tr>
    <tr>
      <td class="form-label">Title</td>
      <td><input type="text" name="Title"></td>
    </tr>
    <tr>
      <td colspan="2">
        <div class="form-element">
          <input type="submit" value="add role manager"/>
        </div>
      </td>
    </tr>
  </table>
</form>

