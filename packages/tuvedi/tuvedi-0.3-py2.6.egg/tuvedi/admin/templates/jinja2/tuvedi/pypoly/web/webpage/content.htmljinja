{% for item in content %}
	<div class="box ui-dialog ui-widget ui-widget-content ui-corner-all">
		<div class="head ui-dialog-titlebar ui-widget-header ui-corner-all ui-helper-clearfix">
			<span class="ui-dialog-title">{{item.title}}</span>
		</div>
		<div class="content">
			{{item()}}
		</div>
	</div>
{% endfor %}
