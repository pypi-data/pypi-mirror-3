[
    {% for hol in holidays %}
    {
        "name": "{{ hol.name }}",
        "date": "{{ hol.date|date:"Y-m-d"}}",
        "country": "{{ hol.country.code }}",
        "provinces": {% if hol.provinces %}{{ hol.provinces }}{% else %}null{% endif %}
    }{% if not loop.last %},{% endif %}
    {% endfor %}
]