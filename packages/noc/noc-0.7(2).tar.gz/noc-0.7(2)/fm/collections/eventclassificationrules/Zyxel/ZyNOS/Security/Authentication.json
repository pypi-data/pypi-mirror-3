[
    {
        "name": "Zyxel | ZyNOS | Security | Authentication | Login (SYSLOG)",
        "description": "Login successful",
        "event_class__name": "Security | Authentication | Login",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Zyxel\\.ZyNOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "auth(entication)?: User (?P<user>.+) login$"
            }
        ]
    },

    {
        "name": "Zyxel | ZyNOS | Security | Authentication | Login Failed (SYSLOG)",
        "description": "Login failed",
        "event_class__name": "Security | Authentication | Login Failed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Zyxel\\.ZyNOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "auth(entication)?: \\S+ authentication failure \\[username: (?P<user>.+)\\]$"
            }
        ]
    },

    {
        "name": "Zyxel | ZyNOS | Security | Authentication | Logout (SYSLOG)",
        "description": "Logout successful",
        "event_class__name": "Security | Authentication | Logout",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Zyxel\\.ZyNOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "auth(entication)?: User (?P<user>.+) logout$"
            }
        ]
    }
]
