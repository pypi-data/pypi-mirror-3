<?xml version="1.0" encoding="UTF-8"?><!-- test that datamodel values can be specified by  namelist.
test240sub1 will return success ifits Var1 is set to 1, failure otherwise.  --><scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" initial="s0" datamodel="python">
<datamodel>
  <data id="Var1" expr="1"/>
  </datamodel>
     
<state id="s0">
  <onentry>
    <send event="timeout" delay="2s"/>
    </onentry>
   
     <invoke type="http://www.w3.org/TR/scxml/" src="file:test244sub1.scxml" namelist="Var1"/>
     <transition event="success" target="pass"/>
     <transition event="*" target="fail"/>
   </state>
 
<final id="pass"><onentry><log label="Outcome" expr="'pass'"/></onentry></final>
<final id="fail"><onentry><log label="Outcome" expr="'fail'"/></onentry></final>
</scxml>