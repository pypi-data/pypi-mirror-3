#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'p2v==2.2.7','console_scripts','convert-p2v-xen'
__requires__ = 'p2v==2.2.7'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('p2v==2.2.7', 'console_scripts', 'convert-p2v-xen')()
    )
