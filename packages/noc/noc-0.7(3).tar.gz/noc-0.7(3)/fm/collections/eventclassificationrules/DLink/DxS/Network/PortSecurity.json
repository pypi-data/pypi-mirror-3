[
    {
        "name": "DLink | DxS | Network | Port Security | Port Security Violation #1 (SYSLOG)",
        "description": "WARN: Port security violation (port: 14, MAC: 48-5B-39-47-0C-4B)",
        "event_class__name": "Network | Port Security | Port Security Violation",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:WARN:|WARN\\(4\\)) Port security violation \\([Pp]ort: (?P<interface>\\S+), MAC: (?P<mac>\\S+)\\)$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | Port Security | Port Security Violation #2 (SYSLOG)",
        "description": "WARN: Port Security Violation (MAC: B4-82-FE-C2-A3-6A, Port: 8)",
        "event_class__name": "Network | Port Security | Port Security Violation",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:WARN:|WARN\\(4\\)) Port Security Violation \\(MAC: (?P<mac>\\S+), Port: (?P<interface>\\S+)\\)$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | Port Security | Port Security Violation #3 (SYSLOG)",
        "description": "WARN(4) Port security violation mac addrss 04-35-46-25-E2-2F on locking address full port 21",
        "event_class__name": "Network | Port Security | Port Security Violation",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:WARN:|WARN\\(4\\)) Port security violation mac (?:addrss|address) (?P<mac>\\S+) on locking address full port (?P<interface>\\S+)"
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | Port Security | Port Security Violation (SNMP)",
        "description": "DGS3612-L2MGMT-MIB::swL2PortSecurityViolationTrap",
        "event_class__name": "Network | Port Security | Port Security Violation",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^D[EG]S-?\\d{4}.+-MIB::sw(?:L2PortSecurityViolationTrap|l2NotifyPortSecurity.+)$"
            },
            {
                "key_re": "^D[EG]S-?\\d{4}.+-MIB::swL2PortSecurityPortIndex(\\.\\d+)?$",
                "value_re": "^(?P<interface>\\S+)$"
            },
            {
                "key_re": "^D[EG]S-?\\d{4}.+-MIB::swL2PortSecurityViolationMac(\\.\\d+)?$",
                "value_re": "^(?P<mac__bin_to_mac>.+)$"
            }
        ]
    }
]
