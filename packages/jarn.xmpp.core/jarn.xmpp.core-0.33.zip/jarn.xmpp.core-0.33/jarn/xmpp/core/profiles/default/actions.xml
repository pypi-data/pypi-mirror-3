<?xml version="1.0"?>
<object name="portal_actions" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="user">
   <object name="xmpp-profile" meta_type="CMF Action" i18n:domain="jarn.xmpp.core"
           insert-before="logout">
    <property name="title" i18n:translate="">Following</property>
    <property name="description"></property>
    <property name="url_expr">string:xmpp-personal-profile</property>
    <property name="link_target"></property>
    <property name="icon_expr"></property>
    <property name="available_expr">python:member is not None</property>
    <property name="permissions">
     <element value="View"/>
    </property>
    <property name="visible">True</property>
   </object>
 </object>
</object>
