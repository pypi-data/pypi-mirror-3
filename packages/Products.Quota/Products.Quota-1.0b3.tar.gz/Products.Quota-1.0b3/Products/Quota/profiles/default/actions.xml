<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object_buttons" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="add_quota" meta_type="CMF Action" i18n:domain="quota">
   <property name="title" i18n:translate="">Add quota</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/@@make_quota_aware</property>
   <property name="icon_expr"></property>
   <property name="available_expr">not: context/quota_awareness/is_quota_aware</property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="rm_quota" meta_type="CMF Action" i18n:domain="quota">
   <property name="title" i18n:translate="">Remove quota</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/@@rm_quota_awareness</property>
   <property name="icon_expr"></property>
   <property name="available_expr">context/quota_awareness/is_quota_aware</property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="object" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="quota" meta_type="CMF Action" i18n:domain="quota">
   <property name="title" i18n:translate="">Quota</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/@@quota</property>
   <property name="icon_expr"></property>
   <property name="available_expr">context/quota_awareness/is_quota_aware</property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
