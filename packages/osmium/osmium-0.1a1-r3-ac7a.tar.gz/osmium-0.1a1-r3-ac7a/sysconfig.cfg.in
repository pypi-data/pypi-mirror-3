[general]
package = @PACKAGE@

[libs]
osmium = @OSMIUM@
osmium_cflags = @OSMIUM_CPPFLAGS@

expat = @HAVE_EXPAT@
expat_cflags = @EXPAT_CFLAGS@
expat_libs = @EXPAT_LIBS@

osmdefault = @OSMDEFAULT@
osmdefault_cflags = @OSMDEFAULT_CFLAGS@
osmdefault_libs   = @OSMDEFAULT_LIBS@

zlib = @ZLIB@
protobuf = @PROTOBUF@
xml2 = @XML2@

osmpbf = yes
osmpbf_cflags = -Ilibosmpbf
osmpbf_libs = -Llibosmpbf -losmpbf_pic
