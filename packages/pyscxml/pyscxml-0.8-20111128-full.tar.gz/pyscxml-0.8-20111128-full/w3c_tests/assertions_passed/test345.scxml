<?xml version="1.0" encoding="UTF-8"?><!-- test that _event.data field is left unbound if event contained illegal data --><scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" initial="s0" datamodel="python">

  
<state id="s0">
  
   <onentry>
     <send event="event1">
       <conf:illegalcontent/>
       </send>
       <send event="timeout" delay="10s"/>
     </onentry>
  <transition event="event1" cond="not _event.data" target="pass"/>
  <transition event="event1" target="fail"/> 
  <transition event="timeout" target="fail"/>
</state>
  
   <final id="pass"><onentry><log label="Outcome" expr="'pass'"/></onentry></final>
   <final id="fail"><onentry><log label="Outcome" expr="'fail'"/></onentry></final>

</scxml>