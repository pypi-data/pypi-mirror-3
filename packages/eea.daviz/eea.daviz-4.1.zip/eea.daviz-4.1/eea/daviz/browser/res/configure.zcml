<configure xmlns:browser="http://namespaces.zope.org/browser"
  i18n_domain="eea.daviz">

  <browser:page
    for="*"
    name="daviz-view.js"
    class=".javascript.ViewJavascript"
    permission="zope.Public"
    />

  <browser:page
    for="*"
    name="daviz-edit.js"
    class=".javascript.EditJavascript"
    permission="zope.Public"
    />

  <browser:page
    for="*"
    name="daviz-view.css"
    class=".css.ViewCSS"
    permission="zope.Public"
    />

  <browser:page
    for="*"
    name="daviz-edit.css"
    class=".css.EditCSS"
    permission="zope.Public"
    />

</configure>
