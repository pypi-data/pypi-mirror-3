<integral type='phrasegroup'>
       <properties>
                 <property name='wordlike' value='True' type='bool' />
         </properties>
       <entities>
                 <si_cell type='phrase' size_scaling='1.5' padding='0 0 4 0' lead='v' >
                           <properties>
                                     <property name='deletable' value='3' type='int' />
                                     <property name='enterable' value='False' type='bool' />
                                     <property name='attachable' value='False' type='bool' />
                             </properties>
                           <entities>
                                     <Symbol0 type='symbol' shape='∫' name='int_sym' ink='True' italic='True' >
                                               <properties>
                                                         <property name='attachable' value='False' type='bool'/>
                                                 </properties>
                                       </Symbol0>
                             </entities>
                   </si_cell>
                 <col0 type='phrasegroup'>
                           <properties>
                                     <property name='deletable' value='3' type='int' />
                                     <property name='highlight_group' value='False' type='bool' />
                                     <property name='always_recalc' value='True' type='bool' />
                             </properties>
                           <entities>
                                     <VerticalSpacer type='verticalspacer' tied_to='int_sym' scaling='1.2' subtract_other_children='True' />
                                     <fr_cell type='phrase' size_scaling='0.4' row='1' target='from' />
                                     <Space width='0.25' height='0.1' row='-1' />
                                     <to_cell type='phrase' size_scaling='0.4' row='-1' target='to' />
                             </entities>
                           <actions>
                                     <translate h='-0.2' v='0' />
                             </actions>
                           <rows>
                                     <row id='-1' align='l'/>
                                     <row id='1' align='l'/>
                             </rows>
                   </col0>
                 <operand type='phrase' target='operand' recommending='True' operand='0' >
                           <properties>
                                     <property name='deletable' value='3' type='int' />
                             </properties>
                   </operand>
                 <by_phrase type='phrase' padding='5 0 0 0' >
                           <properties>
                                     <property name='enterable' value='False' type='bool' />
                                     <property name='attachable' value='False' type='bool' />
                             </properties>
                           <entities>
                                     <Symbol1 shape='d' type='symbol'>
                                               <properties>
                                                         <property name='attachable' value='False' type='bool'/>
                                                 </properties>
                                       </Symbol1>
                                     <by type='phrase' target='by' />
                             </entities>
                   </by_phrase>
         </entities>
       <sympy>
                 sympy.integrals.Integral(self.get_target('operand').get_sympy(),\
                 (self.get_target('by').get_sympy(), self.get_target('from').get_sympy(), self.get_target('to').get_sympy())) \
                 if len(self['from'])>0 and len(self['to'])>0 else \
                 sympy.integrals.Integral(self['operand'].get_sympy(), self['by'].get_sympy())
         </sympy>
       <latex>
                 "\int^{"+self['to'].to_latex()+"}_{"+self['from'].to_latex()+"} "+self['operand'].to_latex()+\
                 "\,d"+self['by'].to_latex()
         </latex>
       <string>
                 u'∫^{'+self['to'].to_string()+"}_{"+self['from'].to_string()+"} "+self['operand'].to_string()+\
                 u" d"+self['by'].to_string()
         </string>
  <alternatives>integral</alternatives>
  <symbol>∫^</symbol>
  <category>Calculus</category>
  <shortcut>&lt;Alt&gt;i</shortcut>
</integral>
