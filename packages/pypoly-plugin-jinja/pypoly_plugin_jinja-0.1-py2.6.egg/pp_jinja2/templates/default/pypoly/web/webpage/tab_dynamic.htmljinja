<div>
	<script>/*<![CDATA[*/
		$(document).ready(function(){
		$("#{{tabs.id}}").tabs({selected: {{tabs.tab_selected}}});
		});

		/*]]>*/
	</script>
	<div id="{{tabs.id}}">
		<ul class="pypoly-tab">
			{% for tab in tabs %}
				<li class=""><a href="#{{tab.id}}"><span>{{tab.title}}</span></a></li>
			{% endfor %}
		</ul>
		{% for tab in tabs %}
			{{tab()}}
		{% endfor %}
	</div>
</div>
