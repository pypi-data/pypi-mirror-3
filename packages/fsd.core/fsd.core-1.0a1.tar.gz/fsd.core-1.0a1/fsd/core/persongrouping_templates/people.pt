<ul tal:repeat="personTuple view/people">
    <li tal:define="person python:personTuple[0];
                    groupdata python:personTuple[1]">
        <a tal:attributes="href person/absolute_url" 
           tal:content="person/Title" />
        <tal:groupdata tal:condition="groupdata" tal:replace="structure groupdata/@@inline" />
    </li>
</ul>