[
    {
        "name": "Environment | UPS | Input Power Failed (SNMP)",
        "description": "XPPC-MIB::ppc",
        "event_class__name": "Environment | UPS | Input Power Failed",
        "preference": 2000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^XPPC-MIB::ppc$"
            },
            {
                "key_re": "^XPPC-MIB::ppc$",
                "value_re": "^UPS has switched to battery power.$"
            }
        ]
    },

    {
        "name": "Environment | UPS | Input Power OK (SNMP)",
        "description": "XPPC-MIB::ppc",
        "event_class__name": "Environment | UPS | Input Power OK",
        "preference": 2000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^XPPC-MIB::ppc$"
            },
            {
                "key_re": "^XPPC-MIB::ppc$",
                "value_re": "^Utility power has been restored.$"
            }
        ]
    }

]