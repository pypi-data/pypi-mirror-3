<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      tal:omit-tag="1"
      i18n:domain="horae.dashboard">
  <ul class="dashboards button-group noMarker" tal:condition="viewlet.dashboards">
    <li tal:repeat="dashboard viewlet.dashboards">
      <a href="" class="button button-discreet button-alternative"
         tal:attributes="href dashboard['url']"
         tal:content="dashboard['name']" />
    </li>
  </ul>
</html>