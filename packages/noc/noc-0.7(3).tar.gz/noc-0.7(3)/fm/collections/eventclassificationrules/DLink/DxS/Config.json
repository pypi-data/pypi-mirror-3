[
    {
        "name": "DLink | DxS | Config | Config Changed #1 (SYSLOG)",
        "description": "INFO: Configuration saved to flash (Username: admin, IP: <ip>)",
        "event_class__name": "Config | Config Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:INFO:|INFO\\(6\\)) (Unit \\d+, )?Configuration (and log )*saved to flash \\(Username: (?P<user>.+), IP: \\S+.+\\)$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Config | Config Changed #2 (SYSLOG)",
        "description": "INFO(6) Configuration and log saved to flash (Username: fenomen_krog)",
        "event_class__name": "Config | Config Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:INFO:|INFO\\(6\\)) Configuration (and log )*saved to flash \\(Username: (?P<user>.+)\\)$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Config | Config Changed (SNMP)",
        "description": "AGENT-GENERAL-MIB::agentNotifFirmware",
        "event_class__name": "Config | Config Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^AGENT-GENERAL-MIB::agentNotifFirmware$"
            },
            {
                "key_re": "^AGENT-GENERAL-MIB::agentCfgOperate$",
                "value_re": "^save$"
            },
            {
                "key_re": "^AGENT-GENERAL-MIB::agentLoginUserName$",
                "value_re": "^(?P<user>.+)$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Config | Config Uploaded Successfully (SYSLOG)",
        "description": "INFO: Configuration successfully uploaded (Username: admin, IP: 172.16.0.1)",
        "event_class__name": "Config | Config Uploaded Successfully",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:INFO:|INFO\\(6\\)) Configuration successfully uploaded \\(Username: (?P<user>.+), IP: (?P<ip>\\S+)\\)$"
            }
        ]
    }
]