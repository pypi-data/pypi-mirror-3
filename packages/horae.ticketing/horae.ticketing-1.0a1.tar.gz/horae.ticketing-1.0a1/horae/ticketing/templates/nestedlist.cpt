<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      tal:omit-tag="1"
      i18n:domain="horae.ticketing">
<h1 class="firstHeading"
    tal:condition="viewlet.caption"
    tal:content="viewlet.caption">
  Title
</h1>
<ol class="nestedlist noMarker"
    tal:condition="viewlet.list">
  <tal:recurse replace="structure viewlet.recurse(viewlet.list)" />
</ol>
<p class="discreet"
   tal:condition="not viewlet.list and viewlet.empty_message"
   tal:content="viewlet.empty_message">
  Empty
</p>
</html>
