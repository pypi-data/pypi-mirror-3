#: ATGoogleVideo/skins/atgooglevideo_templates/googlevideo_view.pt
msgid "flash_required"
msgstr "να έχετε εγκαταστήσει το πρόγραμμα Adobe Flash Player (έκδοση 9,0,28,0 και πάνω) ${download}"

#. Default: "download it now free"
#: ATGoogleVideo/skins/atgooglevideo_templates/googlevideo_view.pt
msgid "flash_download"
msgstr "κατεβάστε το δωρεάν"

