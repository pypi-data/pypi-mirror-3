<tal:block tal:define="hid view/getHighlighterId">
<iframe tal:condition="hid"
	src="http://highlighter.com" 
	width="100%"
	height="300"
	tal:attributes="src string:http://highlighter.com/analytics/?id=${view/getHighlighterId};">
  <p>Your browser does not support iframes.</p>
</iframe>
<p tal:condition="not: hid">Highlighter integration is not currently enabled.</p>
</tal:block>
