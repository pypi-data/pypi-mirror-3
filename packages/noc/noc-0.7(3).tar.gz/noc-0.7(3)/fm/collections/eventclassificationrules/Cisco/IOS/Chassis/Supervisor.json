[
    {
        "name": "Cisco | IOS | Chassis | Supervisor | Supervisor Down #1 (SYSLOG)",
        "description": "%C4K_REDUNDANCY-3-SIMPLEX_MODE: The peer Supervisor has been lost",
        "event_class__name": "Chassis | Supervisor | Supervisor Down",
        "preference": 1000,
        "vars": [
            {
                "name": "name",
                "value": "Supervisor"
            }
        ],
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%(?:C4K_)?REDUNDANCY-3-SIMPLEX_MODE: The peer Supervisor has been lost"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Chassis | Supervisor | Supervisor Up (SYSLOG)",
        "description": "%C4K_REDUNDANCY-6-DUPLEX_MODE: The peer Supervisor has beendetected",
        "event_class__name": "Chassis | Supervisor | Supervisor Up",
        "preference": 1000,
        "vars": [
            {
                "name": "name",
                "value": "Supervisor"
            }
        ],
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^Cisco\\.IOS$"
            },
            {
                "key_re": "^message$",
                "value_re": "%(?:C4K_)?REDUNDANCY-6-DUPLEX_MODE: The peer Supervisor has beendetected-6-DUPLEX_MODE: The peer Supervisor has been\\s*detected"
            }
        ]
    },

    {
        "name": "Cisco | IOS | Chassis | Supervisor | Supervisor Down #2 (SYSLOG)",
        "description": "%REDUNDANCY-3-STANDBY_LOST: Standby processor fault (PEER_DOWN_INTERRUPT)",
