<?xml version="1.0"?>
<object name="BastionMerchantService"
   meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title">BastionMerchantService</property>
 <property name="description">BastionMerchantService content type.</property>
 <property name="content_icon">merchant.gif</property>
 <property name="content_meta_type">BastionMerchantService</property>
 <property name="product">BastionBanking</property>
 <property name="factory"></property>
 <property name="immediate_view">bastionmerchant_view</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="bastionmerchant_view"/>
 <alias from="view" to="(selected layout)"/>
 <alias from="edit" to="bastionmerchant_edit_form"/>
 <alias from="pay" to="bastionmerchant_pay_form"/>
 <alias from="payments" to="bastionmerchant_payments_form"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}" visible="True">
  <permission value="View"/>
 </action>
 <action title="Payments" action_id="payments" category="object" condition_expr=""
    url_expr="string:${object_url}/payments" visible="True">
  <permission value="BastionBanking: Operate"/>
 </action>
 <action title="Pay" action_id="pay" category="object" condition_expr=""
    url_expr="string:${object_url}/pay" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="BastionBanking: Manage"/>
 </action>
</object>
