<?xml version="1.0"?>
<object name="portal_javascripts" meta_type="JavaScripts Registry">

  <!-- http support  -->
  <javascript id="http://maps.google.com/maps/api/js?sensor=false"
              compression="none"
              cookable="False"
              enabled="True"
              expression="python:not context.absolute_url().startswith('https')"
              inline="False"/>

  <!-- https support  -->
  <javascript id="https://maps-api-ssl.google.com/maps/api/js?sensor=false"
              compression="none"
              cookable="False"
              enabled="True"
              expression="python:context.absolute_url().startswith('https')"
              inline="False"/>

  <javascript cacheable="True"
              compression="safe"
              cookable="True"
              enabled="True"
              expression=""
              id="++resource++googlemaps.js"
              inline="False"/>

  <!-- remove old js registrations -->
  <javascript id="http://maps.google.com/maps/api/js?v=3.3&amp;sensor=false" remove="True"/>
  <javascript id="https://maps.google.com/maps/api/js?v=3.3&amp;sensor=false" remove="True"/>

</object>
