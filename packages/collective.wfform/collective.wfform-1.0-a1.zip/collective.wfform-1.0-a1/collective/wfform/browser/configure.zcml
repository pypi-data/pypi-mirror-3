<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="collective.wfform">

  <browser:resourceDirectory
      name="collective.wfform.javascripts"
      directory="javascripts"
      layer=".interfaces.ICollectiveWfform"
      />

  <browser:page
      name="workflow_transition_form"
      for="*"
      class=".form.WorkflowTransitionForm"
      permission="zope2.View"
      />

</configure>
