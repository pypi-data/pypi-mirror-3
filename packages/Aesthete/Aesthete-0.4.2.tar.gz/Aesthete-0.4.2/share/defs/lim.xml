<lim latex_name="\lim">
  <mes>
    <me name="entity" />
    <me name="phrase" />
    <me name="phrasegroup" />
    <me name="lim" />
  </mes>
  <characteristics>
    <ch name="_bodmasable" />
  </characteristics>
  <inherited_properties>
    <ip name="font_size" type="float" value="45.0" />
    <ip name="bold" type="bool" value="False" />
    <ip name="font_size_coeff" type="float" value="1.0" />
    <ip name="rgb_colour" type="tuple">
      <ti type="float" value="0.0" />
      <ti type="float" value="0.0" />
      <ti type="float" value="0.0" />
    </ip>
    <ip name="font_size_combined_coeff" type="float" value="1.0" />
  </inherited_properties>
  <properties>
    <property name="phraser" type="str" value="expression" />
    <property name="highlight_group" type="bool" value="True" />
    <property name="name" type="str" value="lim" />
    <property name="enterable" type="bool" value="False" />
  </properties>
  <entities>
    <phrase_0 type="phrase">
      <properties>
        <property name="enterable" type="bool" value="False" />
      </properties>
      <entities>
        <word_1 type="word">
          <properties>
            <property name="enterable" type="bool" value="False" />
          </properties>
          <entities>
            <symbol_0 ink="False" shape="l" type="symbol">
              <properties>
                <property name="italic" type="bool" value="False" />
              </properties>
            </symbol_0>
            <symbol_1 ink="False" shape="i" type="symbol">
              <properties>
                <property name="italic" type="bool" value="False" />
              </properties>
            </symbol_1>
            <symbol_2 ink="False" shape="m" type="symbol">
              <properties>
                <property name="italic" type="bool" value="False" />
              </properties>
            </symbol_2>
          </entities>
        </word_1>
        <phrase_2 row="1" scaling="0.5" type="phrase">
          <properties>
            <property name="enterable" type="bool" value="False" />
          </properties>
          <entities>
            <space_0 height="0.1" type="space" width="0.1">
              <properties>
                <property name="show_decorated" type="bool" value="True" />
                <property name="attachable" type="bool" value="False" />
                <property name="blank" type="bool" value="True" />
              </properties>
            </space_0>
            <phrase_4 target="sym" type="phrase">
              <properties>
                <property name="deletable" type="int" value="2" />
                <property name="enterable" type="bool" value="False" />
              </properties>
            </phrase_4>
            <word_2 type="word">
              <properties>
                <property name="enterable" type="bool" value="False" />
              </properties>
              <entities>
                <symbol_3 ink="False" shape="→" type="symbol">
                  <properties>
                    <property name="italic" type="bool" value="False" />
                  </properties>
                </symbol_3>
              </entities>
            </word_2>
            <space_1 height="0.1" type="space" width="0.1">
              <properties>
                <property name="show_decorated" type="bool" value="True" />
                <property name="attachable" type="bool" value="False" />
                <property name="blank" type="bool" value="True" />
              </properties>
            </space_1>
            <phrase_5 target="to" type="phrase">
              <properties>
                <property name="deletable" type="int" value="2" />
                <property name="enterable" type="bool" value="False" />
              </properties>
            </phrase_5>
          </entities>
        </phrase_2>
      </entities>
      <rows>
        <row align="c" id="0" />
        <row align="c" id="1" />
      </rows>
    </phrase_0>
    <space_2 height="0.1" type="space" width="0.1">
      <properties>
        <property name="show_decorated" type="bool" value="True" />
        <property name="attachable" type="bool" value="False" />
        <property name="blank" type="bool" value="True" />
      </properties>
    </space_2>
    <phrase_3 target="expression" type="phrase">
      <properties>
        <property name="deletable" type="int" value="2" />
        <property name="enterable" type="bool" value="False" />
      </properties>
    </phrase_3>
  </entities>
  <targets>
    <target name="to">
      <properties>
        <property name="deletable" type="int" value="2" />
      </properties>
    </target>
    <target name="expression">
      <properties>
        <property name="deletable" type="int" value="2" />
      </properties>
    </target>
    <target name="sym">
      <properties>
        <property name="deletable" type="int" value="2" />
      </properties>
    </target>
  </targets>
  <latex>"lim_{"+str(self["sym"].to_string())+" \\to "+str(self["to"].to_string())+"} "+str(self["expression"].to_string())</latex>
  <sympy>limits.limit(self["expression"].get_sympy(), self["sym"].get_sympy(), self["to"].get_sympy())</sympy>
  <string>"lim_{"+str(self["sym"].to_string())+"→"+str(self["to"].to_string())+"}{"+str(self["expression"].to_string())+"}"</string>
  <symbol>lim</symbol>
  <category>Functions</category>
</lim>
