<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="JS Jamba Theme">

 <object name="jsjamba_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.jsjamba:skins/jsjamba_images"/>
 <object name="jsjamba_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.jsjamba:skins/jsjamba_templates"/>
 <object name="jsjamba_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.jsjamba:skins/jsjamba_styles"/>

 <skin-path name="JS Jamba Theme" based-on="Plone Default">
 </skin-path>
 
 <skin-path name="*">
  <layer name="jsjamba_images"
     insert-after="custom"/>
  <layer name="jsjamba_templates"
     insert-after="custom"/>
  <layer name="jsjamba_styles"
     insert-after="custom"/>
 </skin-path>

</object>
