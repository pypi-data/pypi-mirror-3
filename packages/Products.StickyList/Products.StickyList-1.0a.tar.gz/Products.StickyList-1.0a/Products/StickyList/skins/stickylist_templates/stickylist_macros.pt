<div metal:define-macro="listing" class="stickylistListing">
<metal:block tal:define="stickylist python:request.get('stickylist')">
<h4 tal:content="stickylist/Title"></h4>
<ul>
	<li tal:repeat="item python:stickylist.get_items()">
		<a tal:attributes="href item/absolute_url"
			tal:content="item/Title">Tittel</a>
	</li>
</ul>
</metal:block>
</div>
