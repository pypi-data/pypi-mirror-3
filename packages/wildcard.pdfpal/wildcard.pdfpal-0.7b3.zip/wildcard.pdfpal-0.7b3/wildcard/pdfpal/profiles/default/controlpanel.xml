<?xml version="1.0"?>
<object name="portal_controlpanel" 
        meta_type="Plone Control Panel Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n" 
        i18n:domain="wildcard.pdfpal">

	<configlet title="PDF Pal" action_id="pdf-pal"
    appId="wildcard.pdfpal" category="Products" condition_expr=""
    url_expr="string:${portal_url}/@@pdfpal-config"
    icon_expr="string:++resource++pdfpal.files/application-pdf.png"
    visible="True" i18n:attributes="title">
    <permission>Manage Portal</permission>
  </configlet>
  
</object>
