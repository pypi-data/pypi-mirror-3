<tal:window i18n:domain="collective.externalsnippet"
    tal:define="source view/getSource">
    
    <div class="externalSnippet" tal:content="structure source"/>
    
    <dl class="portalMessage warning" tal:condition="not:source">
        <dt i18n:translate="Warning">Warning</dt>
        <dd tal:condition="view/expression"
            i18n:translate="">No matching content found in the fetched page.</dd>
        
        <dd tal:condition="not:view/expression"
            i18n:translate="">No visible data fetched.</dd>
    </dl>
</tal:window>