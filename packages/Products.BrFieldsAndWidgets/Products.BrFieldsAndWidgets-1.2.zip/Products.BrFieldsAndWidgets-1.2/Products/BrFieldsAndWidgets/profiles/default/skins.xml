<?xml version="1.0"?>
<object name="portal_skins">
 <object name="BrF_images" meta_type="Filesystem Directory View"
    directory="Products.BrFieldsAndWidgets:skins/BrF_images"/>
 <object name="BrF_templates" meta_type="Filesystem Directory View"
    directory="Products.BrFieldsAndWidgets:skins/BrF_templates"/>

 <skin-path name="*">
     <layer name="BrF_images" insert-after="custom"/>
     <layer name="BrF_templates" insert-after="BrF_images"/>
 </skin-path>
</object>
