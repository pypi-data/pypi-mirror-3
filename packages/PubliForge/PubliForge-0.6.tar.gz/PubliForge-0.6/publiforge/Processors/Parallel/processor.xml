<publiforge version="1.0">
  <processor>

    <label xml:lang="en">Parallel processing</label>
    <label xml:lang="fr">Traitements parallèles</label>

    <description xml:lang="en">
      Launch several processings in parallel.
    </description>
    <description xml:lang="fr">
      Lancement de plusieurs traitements en parallèle.
    </description>

    <engine>publiforge.lib.engine.parallel</engine>

    <variables>
      <group>
        <label xml:lang="en">Processing list</label>
        <label xml:lang="fr">Liste des traitements</label>

        <var name="processing1" type="processing">
          <label xml:lang="en">Processing n°1</label>
          <label xml:lang="fr">Traitement n° 1</label>
        </var>
        
        <var name="processing2" type="processing">
          <label xml:lang="en">Processing n°2</label>
          <label xml:lang="fr">Traitement n° 2</label>
        </var>
        
        <var name="processing3" type="processing">
          <label xml:lang="en">Processing n°3</label>
          <label xml:lang="fr">Traitement n° 3</label>
        </var>

        <var name="processing4" type="processing">
          <label xml:lang="en">Processing n°4</label>
          <label xml:lang="fr">Traitement n° 4</label>
        </var>
        
        <var name="processing5" type="processing">
          <label xml:lang="en">Processing n°5</label>
          <label xml:lang="fr">Traitement n° 5</label>
        </var>
       </group>
    </variables>

  </processor>
</publiforge>