<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five">

  <five:registerPackage package="." initialize=".initialize" />

  <utility
      factory=".encrypt.PlainEncrypter"
      name="plain"
      />

  <utility
      factory=".encrypt.SHAEncrypter"
      name="sha"
      />

  <utility
      factory=".encrypt.MD5Encrypter"
      name="md5"
      />

  <utility
      factory=".encrypt.SSHAEncrypter"
      name="ssha"
      />

</configure>
