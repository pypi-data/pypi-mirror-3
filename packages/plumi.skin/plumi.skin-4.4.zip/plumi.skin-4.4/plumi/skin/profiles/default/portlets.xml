<?xml version="1.0"?>
<portlets>

     <assignment name="plumi.publish" category="context" key="/"
        manager="plone.leftcolumn" type="plone.portlet.static.Static"
        insert-before="navigation">
        <property name="header">publish</property>
        <property name="more_url"/>
        <property name="hide">False</property>
        <property name="footer"/>
        <property name="text">
            &lt;div id="portlet-publish"&gt;
              &lt;a title="Publish Video" class="internal-link" href="publish"&gt;
                &lt;span&gt;publish&lt;/span&gt;
              &lt;/a&gt;
            &lt;/div&gt;
        </property>
        <property name="omit_border">True</property>
    </assignment>
    <assignment name="navigation" category="context" key="/"
        manager="plone.leftcolumn" type="portlets.Navigation">
    </assignment>

</portlets>
