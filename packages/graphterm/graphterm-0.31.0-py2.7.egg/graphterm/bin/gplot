#!/usr/bin/env python
#

import gtermapi
gtermapi.gplot_setup()

import matplotlib.pyplot as plt

fmt = "png" # or "pdf"

plt.plot([1,2,3,2,3,0])
gtermapi.gplot_savefig(format=fmt)

plt.plot([1,2,3,2,3,1])
gtermapi.gplot_savefig(format=fmt)

for x in range(2,5):
    plt.plot([1,2,3,2,3,x])
    gtermapi.gplot_savefig(format=fmt, overwrite=True)
