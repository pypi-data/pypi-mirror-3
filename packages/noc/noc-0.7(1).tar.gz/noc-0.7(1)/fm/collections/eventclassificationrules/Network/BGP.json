[
    {
        "name": "Network | BGP | Backward Transition (SNMP)",
        "description": "BGP4-MIB::bgpBackwardTransition",
        "event_class__name": "Network | BGP | Backward Transition",
        "preference": 2000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^BGP4-MIB::bgpPeerState\\.(?P<peer>\\S+)$",
                "value_re": "^(?P<state__enum__bgp_state>\\S+)$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^BGP4-MIB::bgpBackwardTransition$"
            }
        ]
    },
    
    {
        "name": "Network | BGP | Established (SNMP)",
        "description": "BGP4-MIB::bgpEstablished",
        "event_class__name": "Network | BGP | Established",
        "preference": 2000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^BGP4-MIB::bgpPeerState\\.(?P<peer>\\S+)$",
                "value_re": "^established$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^BGP4-MIB::bgpEstablished$"
            }
        ]
    }
]
