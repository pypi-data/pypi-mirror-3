[
    {
        "name": "DLink | DGS3100 | Config | Config Changed (SYSLOG)",
        "description": "%Stack-I-STCK-CFG-CHNG: Configuration changed: chain",
        "event_class__name": "Config | Config Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DGS3100$"
            },
            {
                "key_re": "^message$",
                "value_re": "%Stack-I-STCK-CFG-CHNG: Configuration changed: \\S+$"
            }
        ]
    },

    {
        "name": "DLink | DGS3100 | Config | Config Sync Failed (SYSLOG)",
        "description": "%SNMP-W-UNITUPDFAIL: Configuration update of unit 2 failed. Reason:",
        "event_class__name": "Config | Config Sync Failed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DGS3100$"
            },
            {
                "key_re": "^message$",
                "value_re": "%SNMP-W-UNITUPDFAIL: Configuration update of (?P<config>.+) failed. Reason:"
            }
        ]
    },

    {
        "name": "DLink | DGS3100 | Config | Config Changed (SNMP)",
        "description": "RFC1155-SMI::enterprises.89 (RND MIB. Why? I don't know!)",
        "event_class__name": "Config | Config Changed",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DGS3100$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^RFC1155-SMI::enterprises\\.89$"
            },
            {
                "key_re": "^DLINK-3100-DEVICEPARAMS-MIB::rndErrorSeverity\\.0$",
                "value_re": "^warning$"
            },
            {
                "key_re": "^DLINK-3100-DEVICEPARAMS-MIB::rndErrorDesc\\.0$",
                "value_re": "%COPY-N-TRAP: The copy operation was completed successfully"
            }
        ]
    }
]