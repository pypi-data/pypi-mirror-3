<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="object" meta_type="CMF Action Category">
    <property name="title"></property>
    <object name="studygroups" meta_type="CMF Action" i18n:domain="Products.openstudy">
      <property name="title" i18n:translate="">Studygroups</property>
      <property name="description" i18n:translate="">Integrate with OpenStudy study groups.</property>
      <property name="url_expr">string:manage-studygroups</property>
      <property name="icon_expr"/>
      <property name="available_expr">python:object.isPrincipiaFolderish</property>
      <property name="permissions">
	<element value="Modify portal content"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>
