<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="HedDex GreenField Theme">

 <object name="heddex_greenfield_custom_images"
    meta_type="Filesystem Directory View"
    directory="heddex.greenfield:skins/heddex_greenfield_custom_images"/>
 <object name="heddex_greenfield_custom_templates"
    meta_type="Filesystem Directory View"
    directory="heddex.greenfield:skins/heddex_greenfield_custom_templates"/>
 <object name="heddex_greenfield_styles"
    meta_type="Filesystem Directory View"
    directory="heddex.greenfield:skins/heddex_greenfield_styles"/>

 <skin-path name="HedDex GreenField Theme" based-on="Plone Default">
  <layer name="heddex_greenfield_custom_images"
     insert-after="custom"/>
  <layer name="heddex_greenfield_custom_templates"
     insert-after="heddex_greenfield_custom_images"/>
  <layer name="heddex_greenfield_styles"
     insert-after="heddex_greenfield_custom_templates"/>
 </skin-path>

</object>
